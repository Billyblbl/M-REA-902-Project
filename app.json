{
  "expo": {
    "name": "MyTumblr",
    "description": "Projet scolaire dans le cadre du module M-REA-902 Ã  EPITECH",
    "slug": "MyTumblr",
    "owner": "roipeche",
    "privacy": "unlisted",
    "version": "1.0.0",
    "githubUrl": "https://github.com/Billyblbl/M-REA-902-Project",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "We need to access to your photos for sharing purposes !",
          "cameraPermission": "We need to access to your camera for sharing purposes !"
        }
      ]
    ]
  }
}
